{
  "name": "DiscordChroma",
  "version": "0.3.0",
  "description": "Discord Chroma",
  "main": "main.js",
  "author": "DELUUXE",
  "license": "ISC",
  "build": {
    "appId": "com.deluuxe.DiscordChroma",
    "productName": "DiscordChroma",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "build --dir"
  },
  "dependencies": {
    "@chroma-cloud/chromajs": "^0.1.21",
    "auto-launch": "^5.0.5",
    "chroma-js": "^1.4.0",
    "discord-rich-presence": "^0.0.6",
    "discord.js": "^11.4.2",
    "electron-log": "^2.2.17",
    "electron-remote": "^1.3.0",
    "electron-updater": "^2.23.3",
    "node-notifier": "^5.2.1",
    "open": "0.0.5"
  },
  "devDependencies": {
    "electron": "^1.8.8",
    "nan": "^2.11.1"
  }
}
